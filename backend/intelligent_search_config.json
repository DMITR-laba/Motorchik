{
  "relaxation": {
    "enabled": true,
    "max_steps": 5,
    "priority": [
      "color",
      "features",
      "transmission",
      "fuel_type",
      "max_price",
      "min_year",
      "category",
      "brand"
    ],
    "rules": {
